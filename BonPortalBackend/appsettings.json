{
  "ConnectionStrings": {
    "DefaultConnection": "Server=tcp:${DB_SERVER};Initial Catalog=${DB_NAME};Persist Security Info=False;User ID=${DB_USER};Password=${DB_PASSWORD};TrustServerCertificate=True"
  },
  "AzureCommunication": {
    "ConnectionString": "${AZURE_COMMUNICATION_CONNECTION_STRING}",
    "SenderEmail": "${AZURE_COMMUNICATION_SENDER_EMAIL}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}